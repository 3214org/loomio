%md-input-container.md-block
  %label{translate: "time_zone_select.time_zone"}
  %md-select{ng-model: "zone", ng-change: "change()", placeholder: "{{'time_zone_select.local' | translate}}", aria-label: "{{ 'time_zone_select.time_zone' | translate }}"}
    %md-option{ng-repeat: "z in zones track by $index", ng-value: "z.value"}
      {{ z.name }}
